{
    "title": "Brackets|Separator",
    "maintainers": [ "dumch" ],
    "rules": [
        {
            "description": "Right cmd + / -> \\",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "slash",
                        "modifiers": {
                            "mandatory": [ "right_command" ],
                            "optional": [ "any" ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "backslash"
                        }
                    ]
                }
            ]
        },
        {
            "description": "Left cmd + u/p -> (/)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "u",
                        "modifiers": {
                            "mandatory": [ "left_command" ],
                            "optional": [ "any" ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "9",
                            "modifiers": [ "shift" ]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "p",
                        "modifiers": {
                            "mandatory": [ "left_command" ],
                            "optional": [ "any" ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "0",
                            "modifiers": [ "shift" ]
                        }
                    ]
                }
            ]
        },
        {
            "description": "Left cmd + i/o â€” [/]",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "i",
                        "modifiers": {
                            "mandatory": [ "left_command" ],
                            "optional": [ "any" ]
                        }
                    },
                    "to": [ { "key_code": "open_bracket" } ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "o",
                        "modifiers": {
                            "mandatory": [ "left_command" ],
                            "optional": [ "any" ]
                        }
                    },
                    "to": [ { "key_code": "close_bracket" } ]
                }
            ]
        }
    ]
}

