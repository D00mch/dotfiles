{
  "title": "l_cmd + i/o to []",
  "rules": [
    {
      "description": "left cmd + i/o â€”>  cmd + shift + [/]",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "i",
            "modifiers": { "mandatory": ["left_command"] }
          },
          "to": [
              { 
                  "key_code": "open_bracket",
                  "modifiers": [ "shift", "left_command" ]
              }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "o",
            "modifiers": { "mandatory": ["left_command"] }
          },
          "to": [
              { 
                  "key_code": "close_bracket",
                  "modifiers": [ "shift", "left_command" ]
              }
          ]
        }
      ]
    }
  ]
}
