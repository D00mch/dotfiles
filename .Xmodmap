
keycode 0x09 =	Caps_Lock	NoSymbol	Caps_Lock
keycode 0x42 =	Escape		NoSymbol	Escape

keycode 0x45 =	Return		NoSymbol	Return
keycode 0x4A =	Shift_R		NoSymbol	Shift_R
keycode 0x4B =	BackSpace	BackSpace	BackSpace	BackSpace

keycode 0x25 =	Alt_L		ISO_Next_Group	Alt_L		ISO_Next_Group
keycode 0x40 =	Control_L	ISO_Next_Group	Control_L	ISO_Next_Group

clear Shift
clear Lock
clear Control
clear Mod1
clear Mod2
clear Mod3
clear Mod4
clear Mod5

add    Shift   = Shift_L Shift_R Shift_R
add    Lock    = Caps_Lock
add    Control = Control_L Control_R
add    Mod1    = Alt_L Alt_R 0x00CD
add    Mod2    = Num_Lock
add    Mod4    = Super_L Super_R 0x00CE 0x00CF
add    Mod5    = ISO_Level3_Shift Mode_switch
